{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "printWidth": 100,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^react-native$",
    "^expo",
    "^@react-navigation",
    "^@rnmapbox",
    "^@gluestack-ui",
    "<THIRD_PARTY_MODULES>",
    "^@/",
    "^[./]"
  ]
}
