{
  "name": "cirque",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "APP_VARIANT=dev expo start",
    "android": "APP_VARIANT=dev expo run:android",
    "android:device": "APP_VARIANT=dev expo run:android --device",
    "android:build": "eas build --platform android --profile production --auto-submit",
    "ios": "APP_VARIANT=dev expo run:ios",
    "ios:device": "APP_VARIANT=dev expo run:ios --device",
    "ios:build": "eas build --platform ios --profile production --auto-submit",
    "test": "jest",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "check-all": "pnpm format:check && pnpm lint && pnpm typecheck",
    "pre-commit": "pnpm check-all",
    "sync-data": "node ../cirque-data/scripts/sync-data.js",
    "validate-data": "node ../cirque-data/scripts/sync-data.js --check",
    "open-problems-excel": "node ../cirque-data/problems/open-problems-excel.js",
    "csv-to-geojson": "node ../cirque-data/problems/csv-to-geojson.js"
  },
